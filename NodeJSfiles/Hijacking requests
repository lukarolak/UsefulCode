//This is made for service workers
//takes all of the web output and responds with Hello
event.respondWith(
   new Response("Hello")
 );
 
 //Changes the text to be read as html
 event.respondWith(
   new Response("<b>Hello</b>", {
     headers: {'Content-Type':'text/html'}
   })
  
  //added a class
  self.addEventListener('fetch', function(event) {
	// TODO: respond to all requests with an html response
	// containing an element with class="a-winner-is-me".
	// Ensure the Content-Type of the response is "text/html"
 event.respondWith(
   new Response('<strong class="a-winner-is-me"><b>Hello</b></strong>', {
     headers: {'Content-Type':'text/html'},
     //class: {"a-winner-is-me"}
   })
   
 );
 //event has a string at the end
  console.log(event.request)
  if(event.request.url.endsWith('.jpg')){
    event.respondWith(
      fetch('/imgs/dr-evil.gif')
    );
  }
